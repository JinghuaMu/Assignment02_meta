para_distribution <- function(data,paras){
plot_data <- tibble()
for (i in paras){
c <- ensym(i)
para_data <- data %>% group_by({{c}}) %>% summarise(mean_lnRR = mean(lnRR)) %>%
select(mean_lnRR) %>% mutate(parameter = as.character(i))
plot_data <- rbind(para_data,plot_data)
}
